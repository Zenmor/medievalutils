{
  "schemaVersion": 1,
  "id": "zenmedievalutils",
  "version": "${version}",
  "name": "ZenMedievalUtils",
  "description": "some useful stuff for good ol knockoff huabacraft",
  "authors": [
    "Zenmor"
  ],
  "contact": {
    "repo": "https://github.com/Zenmor/medievalutils/"
  },
  "license": "GNU General Public License v3.0",
  "icon": "assets/zenmedievalutils/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.zenmor.zenmedievalutils.client.ZenMedievalUtilsClient"
    ],
    "main": [
      "me.zenmor.zenmedievalutils.ZenMedievalUtils"
    ],
    "modmenu": [
      "me.zenmor.zenmedievalutils.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "medievalutils.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
    "modmenu": ">=5"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/Zenmor/medievalutils"
      }
    }
  }
}
